<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ipuclari_sayfasi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yunus.ototakip.IpuclariSayfasi">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/as1"
        android:id="@+id/imageView13"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/as2"
        android:id="@+id/imageView14"
        android:layout_below="@+id/imageView13"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/as3"
        android:id="@+id/imageView15"
        android:layout_below="@+id/imageView14"
        android:layout_alignParentStart="true" />
</RelativeLayout>
